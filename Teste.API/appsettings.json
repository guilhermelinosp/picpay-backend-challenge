{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Async"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "Connections": {
    "SqlServer": ""
  },
  "Jwt": {
    "Secret": "",
    "Expiry": "",
    "Issuer": "",
    "Audience": ""
  },
  "Cors": {
    "Origins": [
      "*"
    ],
    "Methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE"
    ],
    "Headers": [
      "Content-Type",
      "Authorization"
    ]
  },
  "Swagger": {
    "Title": "picpay-backend-challenge",
    "Version": "v1"
  }
}
